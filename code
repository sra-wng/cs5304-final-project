    "svm = make_pipeline(StandardScaler(), SVC(gamma='auto',kernel='sigmoid'))\n",
    "cv_results = cross_validate(svm, X_selected_40, y, cv=5)\n",
    "\n",
    "print('Feature set: X_selected_40')\n",
    "print('Mean score: ',np.mean(cv_results['test_score']))"
